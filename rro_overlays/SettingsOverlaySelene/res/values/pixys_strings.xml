<?xml version="1.0" encoding="utf-8"?>
<resources>
      <!-- Defines the location of the fingerprint sensor on the device
         0 = back
         0 = back
         1 = front
         1 = front
         2 = left side
         2 = left side
         3 = right side
         3 = right side
    -->
    -->
    <integer name="config_fingerprintSensorLocation">3</integer>
  
   <!-- Enable Battery Health -->
    <bool name="config_supportBatteryHealth">true</bool>

   <!-- Battery Health -->
    <string name="config_batDesCap">/sys/class/power_supply/bms/charge_full_design</string>
    <string name="config_batCurCap">/sys/class/power_supply/battery/charge_counter</string>
    <string name="config_batChargeCycle">/sys/class/power_supply/bms/cycle_count</string>

</resources>
